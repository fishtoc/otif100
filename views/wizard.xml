<odoo>
  <record model="ir.ui.view" id="wizard_form_view">
    <field name="name">wizard.form</field>
    <field name="model">otif100.wizard</field>
    <field name="arch" type="xml">
      <form string="Recalculate release dates">
        <!-- <field name="wo_id"/> -->
        <footer>
          <button name="recalculate" type="object" string="Recalculate dates" class="oe_highlight"/>
          or
          <button special="cancel" string="Cancel"/>
        </footer>
      </form>
    </field>
  </record>

  <act_window id="launch_work_order_wizard" name="Recalculate dates" src_model="otif100.work_order" res_model="otif100.wizard" view_mode="form" target="new" key2="client_action_multi"/>
</odoo>
